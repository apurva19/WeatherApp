{
    "name": "Weather-App",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "It displays the weather at your location",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon20.png",
        "default_title": "Weather-App",
        "default_popup": "index.html"
    },
    "permissions": [
        "https://maxcdn.bootstrapcdn.com/",
        "geolocation"
    ]
}